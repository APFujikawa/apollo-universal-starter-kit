{
  "passPerPreset": true,
  "presets": ["react", "es2015", "stage-2"],
  "env": {
    "development": {
      "plugins": [
        "transform-class-properties",
        "transform-object-rest-spread",
        "transform-object-assign",
        "react-hot-loader/babel"
      ]
    }
  },
  "only": ["*.js", "*.jsx"]
}
