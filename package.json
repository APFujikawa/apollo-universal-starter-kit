{
  "name": "apollo-fullstack-starter-kit",
  "private": true,
  "version": "1.0.0",
  "main": "src/server",
  "engines": {
    "node": "6.2.2",
    "npm": "3.7.3"
  },
  "scripts": {
    "build": "babel-node tools/run build",
    "clean": "rm -rf ./build",
    "start": "if [ \"$NODE_ENV\" = production ]; then node --harmony build/server; else babel-node tools/run watch; fi",
    "test": "mocha --compilers js:babel-core/register --reporter spec --full-trace 'src/**/*.test.js' && npm run lint",
    "lint": "eslint --fix --ext js --ext jsx src",
    "seed": "knex seed:run",
    "migrate": "knex migrate:latest",
    "postinstall": "if [ \"$NODE_ENV\" = production ]; then npm run clean && npm run build && npm run migrate && npm run seed; fi"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:sysgears/apollo-fullstack-starter-kit.git"
  },
  "bugs": {
    "url": "https://github.com/sysgears/apollo-fullstack-starter-kit/issues"
  },
  "homepage": "https://github.com/sysgears/apollo-fullstack-starter-kit",
  "keywords": [
    "apollo",
    "fullstack",
    "starter",
    "graphql",
    "react"
  ],
  "author": "SysGears INC",
  "license": "MIT",
  "dependencies": {
    "aphrodite": "^0.5.0",
    "apollo-client": "^0.4.17",
    "apollo-server": "^0.3.1",
    "body-parser": "^1.15.2",
    "classnames": "^2.2.5",
    "dotenv": "^2.0.0",
    "express": "^4.14.0",
    "graphql": "^0.7.0",
    "graphql-tag": "^0.1.13",
    "graphql-tools": "^0.7.2",
    "graphql-typings": "0.0.1-beta-2",
    "isomorphic-fetch": "^2.2.1",
    "knex": "^0.12.0",
    "minilog": "^3.0.1",
    "react": "^15.3.1",
    "react-addons-update": "^15.3.1",
    "react-apollo": "^0.5.4",
    "react-bootstrap": "^0.30.3",
    "react-dom": "^15.3.1",
    "react-hot-loader": "^3.0.0-beta.5",
    "react-router": "^2.8.1",
    "react-router-redux": "^4.0.5",
    "source-map-support": "^0.4.2",
    "sqlite3": "^3.1.4",
    "thinky": "^2.3.6",
    "webpack": "^1.13.2",
    "webpack-manifest-plugin": "^1.0.1"
  },
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-core": "^6.14.0",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.5",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-inline-import": "^2.0.3",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-transform-class-properties": "^6.11.5",
    "babel-plugin-transform-object-assign": "^6.8.0",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.14.0",
    "bootstrap-sass": "^3.3.7",
    "chai": "^3.5.0",
    "concurrently": "^2.2.0",
    "css-loader": "^0.25.0",
    "eslint": "^3.5.0",
    "eslint-config-airbnb": "^11.0.0",
    "eslint-import-resolver-webpack": "^0.6.0",
    "eslint-plugin-import": "^1.14.0",
    "eslint-plugin-jsx-a11y": "^2.2.1",
    "eslint-plugin-react": "^6.2.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "forever-monitor": "^1.7.0",
    "graphql": "^0.7.0",
    "ignore-loader": "^0.1.1",
    "isomorphic-style-loader": "^1.0.0",
    "json-loader": "^0.5.4",
    "mocha": "^3.0.2",
    "node-sass": "^3.9.3",
    "nodemon": "^1.10.2",
    "postcss-loader": "^0.13.0",
    "react-redux": "^4.4.5",
    "react-transform-hmr": "^1.0.4",
    "resolve-url-loader": "^1.6.0",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "wait-for-port": "0.0.2",
    "webpack-dev-server": "^1.15.1",
    "webpack-hot-middleware": "^2.12.2",
    "webpack-merge": "^0.14.1",
    "webpack-node-externals": "^1.4.3"
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": [
      "airbnb/base",
      "plugin:import/errors",
      "eslint:recommended",
      "plugin:react/recommended"
    ],
    "rules": {
      "no-use-before-define": 0,
      "arrow-body-style": 0,
      "dot-notation": 0,
      "no-console": 0
    },
    "settings": {
      "import/resolver": {
        "webpack": {
          "config": "./tools/webpack.config.js"
        }
      }
    },
    "env": {
      "mocha": true
    },
    "globals": {
      "window": true,
      "document": true,
      "__DEV__": true,
      "__CLIENT__": true,
      "__SERVER__": true
    },
    "plugins": [
      "react"
    ]
  }
}
